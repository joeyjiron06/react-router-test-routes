{
  "name": "react-router-test-routes",
  "version": "1.0.0-beta.8",
  "description": "A utility for react-router get get routes that can be used for testing with vitest.",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "typings": "dist/src/index.d.ts",
  "module": "./dist/src/index.js",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js",
      "import": "./dist/src/index.js"
    },
    "./vite": {
      "types": "./dist/src/vite.d.ts",
      "default": "./dist/src/vite.js",
      "import": "./dist/src/vite.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "react-router",
    "vitest",
    "test",
    "nodejs",
    "react",
    "router",
    "@react-router"
  ],
  "author": "Joey Jiron <joeyjiron06@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@react-router/dev": "^7.8.1",
    "@testing-library/react": "^16.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.8.1"
  }
}
